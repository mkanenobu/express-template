  "dependencies": {
    "express": "4.18.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "latest",
    "esbuild": "^0.14.21",
    "nodemon": "2.0.15",
    "npm-run-all": "latest",
    "prettier": "latest",
    "typescript": "^4.5.5",
    "vite": "^2.9.9",
    "vitest": "^0.12.6"
  }
}
